&nbsp; &nbsp; <span class="kw1">print</span> <span class="st0">' --- TRE [%s] ---'</span> % tre.<span class="me1">getTag</span><span class="br0">&#40;</span><span class="br0">&#41;</span><br />
&nbsp; &nbsp; <span class="kw1">for</span> fieldname, field <span class="kw1">in</span> tre:<br />
